@PACKAGE_INIT@

# https://cmake.org/cmake/help/v3.18/module/CMakePackageConfigHelpers.html
include(CMakeFindDependencyMacro)

# set includes and targets
set_and_check(plain_cmake_INCLUDE_DIRS "@PACKAGE_plain_cmake_INCLUDE_DIRS@")
if(NOT TARGET plain_cmake AND NOT plain_cmake_BINARY_DIR)
  include (${CMAKE_CURRENT_LIST_DIR}/plain_cmakeTargets.cmake)
endif()

# load and import targets
find_dependency(Eigen3 3.3)

# labels the packages as found or not
check_required_components(plain_cmake)
